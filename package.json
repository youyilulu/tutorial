{
  "name": "tutorial",
  "version": "1.0.0",
  "description": "tutorial",
  "private": true,
  "dependencies": {
    "egg-scripts": "^2.10.0",
    "egg-ts": "../tutorial_plugins/egg-ts",
    "egg-born-core": "../tutorial_plugins/egg-born-core",
    "egg-born-cache": "../tutorial_plugins/egg-born-cache",
    "egg-born-cache-redis": "../tutorial_plugins/egg-born-cache-redis",
    "midway": "^1.0.0",
    "mysql2": "^2.0.1",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.5",
    "@types/mocha": "^5.2.7",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.8.0",
    "midway-bin": "1",
    "midway-mock": "1",
    "ts-node": "^8.3.0",
    "tslib": "^1.8.1",
    "tslint": "^5.11.0",
    "tslint-midway-contrib": "1",
    "typescript": "^3.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=midway-server-tutorial --framework=midway --ts",
    "stop": "egg-scripts stop --title=midway-server-tutorial",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "create-db": "cross-env NODE_ENV=create-db midway-bin dev --ts"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/youyilulu/tutorial.git"
  },
  "author": "youyilulu",
  "license": "MIT"
}
